@model dotnet_mvc_ef_core.DataContext.CustomerEntity

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Detail Customer</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Gender</th>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
</table>
